<meta property="og:video" content="{{url}}" />
{{#if secureUrl}}<meta property="og:video:secure_url" content="{{secureUrl}}" />{{/if}}
{{#if type}}<meta property="og:video:type" content="{{type}}" />{{/if}}
{{#if width}}<meta property="og:video:width" content="{{width}}" />{{/if}}
{{#if height}}<meta property="og:video:height" content="{{height}}" />{{/if}}